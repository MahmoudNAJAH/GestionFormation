@model IEnumerable<GestionFormation.DTO.UserDTO2>
<br />
<br />
<img src="~/Images/image1.PNG" alt="logo Dawan" />
<h2>Bienvenue chez Dawan !!</h2>


@{
    ViewBag.Title = "Index";
}


<p>
    @Html.ActionLink("Imprimer ma feuille de présence ", "feuillePresence")
</p>

<p>
    
    @Html.ActionLink(" feuille d'évaluation  ", "feuilleEvaluation")
</p>
<link rel="stylesheet" href="~/Content/IndexApprenant.css">


<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Nom)
            </th>
            <th scope="col">
                @Html.DisplayNameFor(model => model.Prenom)
            </th>
            <th scope="col">

                @Html.DisplayNameFor(model => model.Email)
            </th>


        </tr>
    </thead>
    <tr>

        <td>@(((GestionFormation.DTO.UserDTO)Session["userConnected"]).Nom)</td>
        <td>@(((GestionFormation.DTO.UserDTO)Session["userConnected"]).Prenom)</td>

        <td>@(((GestionFormation.DTO.UserDTO)Session["userConnected"]).Email)</td>


    </tr>

</table>
